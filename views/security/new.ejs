<%- include('../partials/header') %>
<%- include('../partials/navbar') %>

    <main class="container">
      <%- include("../partials/flash") %>
      <h1 class="display-6 hd">New Entry</h1>
        <form class="row g-3" action="/security" method="POST">
            <div class="col-md-12">
              <label for="name" class="form-label">Name: </label>
              <input type="text" class="form-control" name="name" id="name" placeholder="Name here">
            </div>
            <div class="col-md-12">
              <label for="phone" class="form-label">Phone: </label>
              <input type="text" class="form-control" name="phone" id="phone" placeholder="Phone Number">
            </div>
            <div class="col-md-12">
              <label for="email" class="form-label">Email: </label>
              <input type="text" class="form-control" name="email" id="email" placeholder="example@abc.com">
            </div>
            <div class="col-12">
              <button type="submit" id="btn" class="btn btn-outline-dark">Check in</button>
            </div>
          </form>
    </main>


    <%- include('../partials/footer') %>